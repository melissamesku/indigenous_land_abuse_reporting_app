reports _form

<%= form_for @report do |form| %>
  Headline: <%= form.text_field :headline, :class => 'testing' %></br>
  Details: <%= form.text_field :details %></br>
  Location: <%= form.text_field :location %></br>
  Upload images, files, documents: <%= form.text_field :image %></br>
  <%= form.hidden_field :user_id, :value => current_user.id %>
  <%= form.submit %>
<% end %>